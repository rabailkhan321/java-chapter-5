//import java.util.Scanner;

public class Comparison{
	
	public static void main(String[] args){
		
		
	System.out.printf("false and false is %b%n", (false&&false));
	System.out.printf("true and false is %b%n", (true&&false));
	System.out.printf("true and true is %b%n", (true&&true));
	System.out.printf("false and true is %b%n", (false&&true));
	
	
	
	System.out.printf("false or false is %b%n", (false||false));
	System.out.printf("true or false is %b%n", (true||false));
	System.out.printf("true or true is %b%n", (true||true));
	System.out.printf("false or true is %b%n", (false||true));
	
	
	
	System.out.printf("false & false is %b%n", (false&false));
	System.out.printf("true & false is %b%n", (true&false));
	System.out.printf("true & true is %b%n", (true&true));
	System.out.printf("false & true is %b%n", (false&true));
	
	
	
	System.out.printf("false | false is %b%n", (false|false));
	System.out.printf("true | false is %b%n", (true|false));
	System.out.printf("true | true is %b%n", (true|true));
	System.out.printf("false | true is %b%n", (false|true));
	
	
	
	
	System.out.printf("false ^ false is %b%n", (false^false));
	System.out.printf("true ^ false is %b%n", (true^false));
	System.out.printf("true ^ true is %b%n", (true^true));
	System.out.printf("false ^ true is %b%n", (false^true));
	
	
	
	System.out.printf(" !false is %b%n", (!false));
	System.out.printf("!true is %b%n", (!true));

	
	
	
	}
}
